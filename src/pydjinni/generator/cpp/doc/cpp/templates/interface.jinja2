{% extends "base.jinja2" %}

{% block content %}
<h2>Member functions:</h2>

{% for method in decl.methods %}
    <pre><code>{{ "static " if method.static }}{{ method.return_type_ref.type_def.cpp.typename if method.return_type_ref else "void" }} {{ method.cpp.name }}(
        {%- for param in method.parameters -%}
            {{ param.cpp.type_spec }} {{ param.cpp.name }}{{ ", " if not loop.last }}
        {%- endfor -%});</code></pre>
    {{ method | comment }}
    <h3>Parameters:</h3>
    <ul>
        {% for param in method.parameters %}
        <li><strong>{{ param.java.name }}</strong> {{ param|comment }}</li>
        {% endfor %}
    </ul>
{% endfor %}
{% endblock %}
